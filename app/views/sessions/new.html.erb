<div class="container-fluid">
<h1>Login</h1>
<%= form_with method: "post", url: "/sessions" do |form| %>
  <div>
    <%= form.label :email %>
    <%= form.email_field :email %>
  </div>
  <div>
    <%= form.label :password %>
    <%= form.password_field :password %>
  </div>
  <div>
    <%= form.submit "Sign In" %>
  </div>
<% end%>

  <a class="btn btn-primary my-2" href="<%= path_to_font new_user_path %>">Sign Up</a>
</div>